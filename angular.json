					"options": {
						"outputPath": "dist/feed",
						"index": "projects/feed/src/index.html",
						"main": "projects/feed/src/main.ts",
						"polyfills": "projects/feed/src/polyfills.ts",
						"tsConfig": "projects/feed/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/feed/src/favicon.ico",
