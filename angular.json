					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "feed:build",
						"extraWebpackConfig": "projects/feed/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/feed/src/test.ts",
						"polyfills": "projects/feed/src/polyfills.ts",
						"tsConfig": "projects/feed/tsconfig.spec.json",
						"karmaConfig": "projects/feed/karma.conf.js",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/feed/src/favicon.ico",
							"projects/feed/src/assets"
						],
						"styles": [
							"projects/feed/src/styles.scss"
						],
						"scripts": []
					}
				}
			}
		},
		"data-store": {
			"projectType": "library",
			"root": "projects/data-store",
			"sourceRoot": "projects/data-store/src",
			"prefix": "lib",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:ng-packagr",
					"options": {
						"project": "projects/data-store/ng-package.json"
					},
					"configurations": {
						"production": {
							"tsConfig": "projects/data-store/tsconfig.lib.prod.json"
						},
						"development": {
							"tsConfig": "projects/data-store/tsconfig.lib.json"
						}
					},
					"defaultConfiguration": "production"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/data-store/src/test.ts",
						"tsConfig": "projects/data-store/tsconfig.spec.json",
						"karmaConfig": "projects/data-store/karma.conf.js"
					}
				}
			}
		}
	},
	"defaultProject": "shell"
}